PROC 0 DEBUG( )
/*******************************************************************/
/*    THIS CLIST ALLOWS ACCESS TO "SMPE" USING LIBDEFS AND ALTLIB  */
/*******************************************************************/
IF &DEBUG = DEBUG THEN +
  CONTROL LIST CONLIST SYMLIST MSG NOFLUSH
ELSE +
  CONTROL NOLIST NOCONLIST NOSYMLIST NOMSG NOFLUSH
    FREE FILE(STEPLIBY)
    ALLOC F(STEPLIBY) DA('SYS1.SGIMLMD0') SHR
    STEPLIB OPEN(STEPLIBY) QUIET
    ALLOC F(SMPTABL) DA(ISPPROF) SHR
    ISPEXEC LIBDEF ISPLLIB EXCLDATA ID('SYS1.SGIMLMD0')
    ISPEXEC LIBDEF ISPPLIB DATASET ID('SYS1.SGIMPENU' PDS.PLIB)
    ISPEXEC LIBDEF ISPMLIB DATASET ID('SYS1.SGIMMENU')
    ISPEXEC LIBDEF ISPTLIB DATASET ID('SYS1.SGIMTENU')
    ISPEXEC LIBDEF ISPSLIB DATASET ID('SYS1.SGIMSENU')
  ISPEXEC SELECT PANEL(SMPE) NEWAPPL(SMPE) PASSLIB
    ISPEXEC LIBDEF ISPLLIB
    ISPEXEC LIBDEF ISPPLIB
    ISPEXEC LIBDEF ISPMLIB
    ISPEXEC LIBDEF ISPTLIB
    ISPEXEC LIBDEF ISPSLIB
    FREE FI(SMPTABL)
    STEPLIB CLOSE QUIET
    FREE FI(STEPLIBY)
    CONTROL NOFLUSH
END
