PROC 0
  CONTROL LIST CONLIST SYMLIST MSG NOFLUSH
FREE  F(SYSPROC)
ALLOC F(SYSPROC) DA('WRKBNCH.ISPF.CLIST' +
                    'SYS1.SISPEXEC'       +
                    'SYS1.SISPCLIB'       +
                    'SYS1.CLIST') SHR
FREE F(IPCSDDIR IPCSPRNT IPCSATTR IPCSPARM)
ALLOC F(IPCSDDIR) DA(TST.IPCSDDIR)
DEL IPCSPRNT
ATTR IPCSATTR RECFM(V B) LRECL(137) BLKSIZE(23476)
ALLOC F(IPCSPRNT) DA(IPCSPRNT) UNIT(DISK) SPACE(5 1) CYLINDERS -
   USING(IPCSATTR)
