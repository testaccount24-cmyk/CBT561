CONTROL LIST MSG
FREE FI(ISPTABL)
ALLOCATE FI(ISPTABL) SHR DA(ISPPROF)
FREE FI(ISPTLIB)
ALLOCATE FI(ISPTLIB) SHR DA(ISPPROF 'SYS1.ISRTLIB' 'SYS1.ISPTLIB')
FREE FI(ISPMLIB)
ALLOCATE FI(ISPMLIB) SHR DA('SYS1.ISRMLIB' 'SYS1.ISPMLIB')
FREE FI(ISPPLIB)
ALLOCATE FI(ISPPLIB) SHR DA('SYS1.ISRPLIB' 'SYS1.ISPPLIB')
FREE FI(ISPSLIB)
ALLOCATE FI(ISPSLIB) SHR DA('SYS1.ISRSLIB' 'SYS1.ISPSLIB')
FREE FI(ISPLLIB)
FREE FI(ISPCTL0)
ALLOCATE FI(ISPCTL0) NEW SP(1 1) CYL UNIT(VIO) +
         LRECL(80) DSORG(PS) RECFM(F B) BLKSIZE(23440)
FREE FI(ISPCTL1)
ALLOCATE FI(ISPCTL1) NEW SP(1 1) CYL UNIT(VIO) +
         LRECL(80) DSORG(PS) RECFM(F B) BLKSIZE(23440)
FREE FI(ISPCTL2)
ALLOCATE FI(ISPCTL2) NEW SP(1 1) CYL UNIT(VIO) +
         LRECL(80) DSORG(PS) RECFM(F B) BLKSIZE(23440)
FREE FI(SYSPROC)
