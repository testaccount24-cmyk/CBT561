/*  THIS IS THE 'ISD ONCALL' CLIST
PROC 1 PROCNAME
CONTROL MSG LIST FLUSH CONLIST SYMLIST
/*CONTROL NOMSG NOLIST NOFLUSH
SET &MODE = 'T'
FREE F(INPUT1,INPUT2,INPUT3,INPUT4,INPUT5,INPUT6,FINCALL,MICSCALL)
FREE F(SYSOUT SYSUDUMP)
ALLOC F(FINCALL) DA('TEST.TST.FINANCE.JOBS(ONCALL)') SHR
ALLOC F(MICSCALL) DA('MICS.PRD.C6.LOCALMOD.CNTL(MICSCALL)') SHR
ALLOC F(INPUT1)  DA('PROD.OWNER.DATA') SHR
ALLOC F(INPUT2)  DA('SYS1.PPOPTION(TSOUSERS)') SHR
ALLOC F(INPUT3)  DA('CLAIMS.OWNER.CNTL(CROSLIST)' -
                    'TEST.TST.PPR.OWNER.CNTL(PPRLIST)' -
                    'TEST.TST.CHIPS.OWNER.CNTL(CHIPLIST)') SHR
ALLOC F(INPUT4)  DA('CLAIMS.OWNER.CNTL(PHONENOS)' -
                    'TEST.TST.PPR.OWNER.CNTL(PHONENOS)' -
                    'TEST.TST.CHIPS.OWNER.CNTL(PHONENOS)') SHR
ALLOC F(INPUT5)  DA('CLAIMS.OWNER.CNTL(EXCEPT)' -
                    'TEST.TST.PPR.OWNER.CNTL(EXCEPT)' -
                    'TEST.TST.CHIPS.OWNER.CNTL(EXCEPT)') SHR
ALLOC F(INPUT6)  DA('CLAIMS.OWNER.CNTL(SUPER)' -
                    'TEST.TST.PPR.OWNER.CNTL(SUPER)' -
                    'TEST.TST.CHIPS.OWNER.CNTL(SUPER)') SHR
ALLOC F(SYSOUT)  DA(*)
ALLOC F(SYSUDUMP) SYSOUT(A)
IF &LENGTH(&PROCNAME) = 7 THEN SET &PROCNAME=&STR(&PROCNAME )
IF &LENGTH(&PROCNAME) = 6 THEN SET &PROCNAME=&STR(&PROCNAME  )
IF &LENGTH(&PROCNAME) = 5 THEN SET &PROCNAME=&STR(&PROCNAME   )
IF &LENGTH(&PROCNAME) = 4 THEN SET &PROCNAME=&STR(&PROCNAME    )
IF &LENGTH(&PROCNAME) = 3 THEN SET &PROCNAME=&STR(&PROCNAME     )
IF &LENGTH(&PROCNAME) = 2 THEN SET &PROCNAME=&STR(&PROCNAME      )
IF &LENGTH(&PROCNAME) = 1 THEN SET &PROCNAME=&STR(&PROCNAME       )
CLR
CALL 'EMVSP.OUTPUT.LOADLIB(RUX2027P)','T&PROCNAME.'
FREE F(INPUT1 INPUT2 INPUT3 INPUT4 INPUT5 INPUT6 SYSOUT SYSUDUMP -
                             FINCALL MICSCALL)
EXIT
